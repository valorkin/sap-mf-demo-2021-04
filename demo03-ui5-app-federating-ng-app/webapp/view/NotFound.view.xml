<mvc:View
	controllerName="com.sap.sapui5POC.controller.NotFound"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">

	<MessagePage
		title="{i18n>notFoundTitle}"
		text="{i18n>notFoundText}"
		icon="sap-icon://document"
		id="page"
		description=""
		showNavButton="true"
		navButtonPress=".onLinkPressed">
		<customDescription>
			<Link id="link" text="{i18n>backToWorklist}" press=".onLinkPressed"/>
		</customDescription>
	</MessagePage>

</mvc:View>